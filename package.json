{
  "name": "incertae-sedis",
  "version": "3.0.0",
  "description": "Experimental testbeds to produce some witty program",
  "scripts": {
    "build": "webpack -p && echo \"build completed - ready for local http static content test \"",
    "build-commit": "rm -f incertae-sedis/bundle.js && rm -f incertae-sedis/bundle.js && webpack -p && mv incertae-sedis/bundle.js ./ && echo \"build completed - ready to commit \"",
    "start": "webpack-dev-server --inline --host 0.0.0.0 --history-api-fallback --progress",
    "start-debug": "rm -f incertae-sedis/bundle.js && webpack-dev-server -d --inline --host 0.0.0.0 --history-api-fallback --progress",
    "chromedbg": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222 http://localhost:8080/incertae-sedis/",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rondinif/incertae-sedis.git"
  },
  "author": "Franco Rondini",
  "license": "BSD-2-Clause",
  "dependencies": {

    "axios": "^0.11.0",
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",

    "react": "^15.4.2",
    "react-dom": "^15.4.2",

    "redux": "^3.0.4",
    "redux-promise": "^0.5.3",

    "react-interactive": "^0.4.0",

    "react-redux": "^4.4.5",

    "react-router": "^3.0.0"    
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",

    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.4.2",

    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
